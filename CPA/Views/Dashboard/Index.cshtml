            function initializeDataTables() {
                console.log("Initializing DataTables");
                $('.tblData').each(function () {
                    if (!$.fn.dataTable.isDataTable($(this))) {
                        $(this).DataTable({
                            "paging": true,
                            "searching": true,
                            "ordering": false,
                            "info": false,
                            "lengthMenu": [[4, -1], ["Summary", "All"]],
                            "columnDefs": [
                                { "orderable": true, "targets": 1 }
                            ],
                            "order": [[1, 'asc']],
                            "autoWidth": false,
                            "language": {
                                "search": "Search in this record:"
                            }
                        });
                    }
                });
            }

            initializeDataTables();

            $('#externalSearch').on('keyup', function () {
                var searchTerm = $(this).val().toLowerCase();
                var tablesMatched = false;

                console.log("Search term: " + searchTerm);

                $('.tab-pane').each(function () {
                    var $tabPane = $(this);
                    var $tables = $tabPane.find('.tblData');

                    $tables.closest('table').hide();
                    $tabPane.hide();

                    $tables.each(function () {
                        if ($.fn.dataTable.isDataTable($(this))) {
                            $(this).DataTable().destroy();
                        }
                    });

                    $tables.each(function () {
                        var nombre = $(this).data('nombre').toLowerCase();
                        var nombreComercial = $(this).data('nombrecomercial').toLowerCase();
                        var parentTable = $(this).closest('table');
                        var parentTabPane = parentTable.closest('.tab-pane');
                        var tableUserId = $(this).data('id');
                        console.log('Table User Id = ', tableUserId);

                        console.log("Checking table with Nombre: " + nombre + " and Nombre Comercial: " + nombreComercial);

                        if (nombre.includes(searchTerm) || nombreComercial.includes(searchTerm)) {
                            console.log("nombre.includes(searchTerm) is true");
                            parentTable.show();
                            parentTabPane.show();
                            tablesMatched = true;

                            // Hide all edit and delete buttons first
                            $('.edit-button, .delete-button').hide();

                            // Show the edit and delete buttons only for the specific user ID
                            $('.edit-button, .delete-button').each(function () {
                                console.log("Button data-id: " + $(this).data('id'));
                                if ($(this).data('id') == tableUserId) {
                                    console.log("Matched");
                                    $(this).show();
                                }
                            });

                            return false; // Exit the each loop once a match is found
                        }
                    });

                    if (tablesMatched) {
                        $tables.filter(':visible').each(function () {
                            if (!$.fn.dataTable.isDataTable($(this))) {
                                $(this).DataTable({
                                    "paging": true,
                                    "searching": true,
                                    "ordering": false,
                                    "info": false,
                                    "lengthMenu": [[4, -1], ["Summary", "All"]],
                                    "columnDefs": [
                                        { "orderable": true, "targets": 1 }
                                    ],
                                    "order": [[1, 'asc']],
                                    "autoWidth": false,
                                    "language": {
                                        "search": "Search in this record:"
                                    }
                                });
                            }
                        });
                    }

                    if (!tablesMatched) {
                        $('.tab-pane').show();
                        console.log("No tables matched. Showing all tab panes.");
                    }

                    if (searchTerm === "") {
                        $('.tab-pane').removeClass('show active');
                        $('#demograficos').addClass('show active');
                        console.log("Search term is empty. Resetting view.");
                    }

                    $tabPane.find('.edit-button:visible, .delete-button:visible').each(function () {
                        console.log("Visible button ID: " + $(this).data('id'));
                    });
                });
            });

            function prioritizeActiveTabTables(activeTabId) {
                console.log("Prioritizing tables in active tab:", activeTabId);
                $('.tab-pane').each(function () {
                    var tabPane = $(this);
                    if (tabPane.attr('id') === activeTabId.substring(1)) {
                        tabPane.prependTo('#dashboardTabsContent');
                        console.log("Active tab pane moved to top:", tabPane.attr('id'));
                    }
                });
            }

            prioritizeActiveTabTables($('#dashboardTabs a.active').attr('href'));
