                body: formData
            })
                .then(response => {
                    if (!response.ok) {
                        return response.text().then(text => { throw new Error(text); });
                    }
                    return response.json();
                })
                .then(data => {
                    if (data.success) {
                        toastr.success('Customer records created successfully!');
                        $('#newCustomerModal').modal('hide');
